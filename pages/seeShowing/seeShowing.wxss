
.wrapper {
  display: flex;
  justify-content: space-around;
  width: 100%;              
  height: 80rpx;
}
.tab-content {
  width: 240rpx;
  height: 76rpx;
  text-align: center;
  line-height: 76rpx;
}
.select {
  border-bottom-width: 2rpx;
  border-bottom-style: solid;
  border-bottom-color: blue;
  color: blue;
}
.noSelect {
  color: #666;
}
.text {
  text-align: center;
  padding: 10rpx 20rpx;
  background-color: #f3f3f3;
  margin-bottom: 20rpx;
}